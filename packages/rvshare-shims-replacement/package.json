{
  "name": "@rvshare/shims-replacement",
  "version": "0.0.1-canary.5",
  "main": "dist/rvshare-shims-replacement.js",
  "files": [
    "dist",
    "shims"
  ],
  "scripts": {
    "prepublish": "microbundle src/index.js -f cjs --no-sourcemap --external none --target node",
    "build": "microbundle build src/index.js -f cjs --no-sourcemap --external none --target node && ncp ./src/shims ./dist/shims"
  },
  "devDependencies": {
    "microbundle": "0.11.0",
    "ncp": "2.0.0"
  }
}
